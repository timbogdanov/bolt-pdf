{
  "name": "timbogdanov/bolt-pdf",
  "description": "High-performance Laravel Blade-to-PDF package using headless Chrome",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0",
    "chrome-php/chrome": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "BoltPdf\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BoltPdf\\BoltPdfServiceProvider"
      ],
      "aliases": {
        "BoltPdf": "BoltPdf\\Facades\\BoltPdf"
      }
    }
  }
}